<div id="muted-topic-settings" class="settings-section" data-name="muted-users-topics">
    <h3>Muted topics</h3>
    <table id="muted_topics_table" class="table">
        <thead>
            <th>{{t "Stream" }}</th>
            <th>{{t "Topic" }}</th>
            <th class="actions">{{t "Actions" }}</th>
        </thead>
        <tbody class="required-text" data-empty="{{t 'You have not muted any topics yet.'}}"></tbody>
    </table>
    <hr />
    <h3>Muted users</h3>
    <table id="muted_users_table" class="table">
        <thead>
            <th>{{t "Users" }}</th>
            <th class="actions">{{t "Actions" }}</th>
        </thead>
        <tbody class="required-text" data-empty="{{t 'You do not have any muted users.'}}"></tbody>
    </table>
</div>
